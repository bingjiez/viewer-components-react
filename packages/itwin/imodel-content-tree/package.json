{
  "name": "@itwin/imodel-content-tree-react",
  "version": "0.3.3",
  "description": "React tree component that shows iModel content",
  "keywords": [
    "Bentley",
    "iModel.js",
    "iModel Content",
    "Tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/viewer-components-react/tree/master/packages/itwin/imodel-content-tree"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/imodel-content-tree.js",
  "typings": "lib/imodel-content-tree",
  "scripts": {
    "build": "npm run -s dual-build && npm run -s copy:assets",
    "dual-build": "npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "copy:assets": "npm run -s copy:public && npm run -s copy:cjs && npm run -s copy:esm",
    "copy:public": "cpx \"./public/**/*\" ./lib/public",
    "copy:cjs": "cpx \"./src/**/*.scss\" ./lib/cjs",
    "copy:esm": "cpx \"./src/**/*.scss\" ./lib/esm",
    "cover": "",
    "lint": "",
    "test": "",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@itwin/appui-abstract": "rc",
    "@itwin/components-react": "rc",
    "@itwin/core-bentley": "rc",
    "@itwin/build-tools": "rc",
    "@itwin/core-common": "rc",
    "@itwin/core-frontend": "rc",
    "@itwin/core-react": "rc",
    "@itwin/presentation-common": "rc",
    "@itwin/presentation-components": "rc",
    "@itwin/presentation-frontend": "rc",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "tslint": "^5.11.0",
    "typescript": "~4.3.0"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "rc",
    "@itwin/components-react": "rc",
    "@itwin/core-bentley": "rc",
    "@itwin/build-tools": "rc",
    "@itwin/core-common": "rc",
    "@itwin/core-frontend": "rc",
    "@itwin/core-react": "rc",
    "@itwin/presentation-common": "rc",
    "@itwin/presentation-components": "rc",
    "@itwin/presentation-frontend": "rc",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "eslintConfig": {
    "extends": "./node_modules/@bentley/build-tools/.eslintrc.js"
  }
}
